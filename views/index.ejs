<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Date Night Wishlist</title>
</head>
<body>
    <h1>Date Night Wishlist</h1>
    <ul>
        <% dates.forEach(date => { %>
            <li>
                <a href="/dates/<%= date._id %>">
                    <span><%= date.location %></span><br></a> 
                    <span><img src="<%= date.img %>"/></span><br>
                <form action="/dates/<%= date._id %>?_method=DELETE" method="POST">
                    <input type="submit" value="delete">
                </form>
                <a href="/dates/<%= date._id %>/edit">Want to make changes?</a>

            </li>
      <% }) %>
    </ul>
    <a href="/dates/new">Add a New Date</a>
</body>
</html>